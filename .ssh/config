Host *
    AddKeysToAgent yes
    #UseKeychain yes
    IdentityFile ~/.ssh/id_rsa
    ServerAliveInterval 120
    TCPKeepAlive no

Host pi
    HostName 192.168.1.95
    IdentityFile ~/.ssh/id_rsa
    User pi
    ForwardX11 yes

Host ciccia
    #HostName 192.168.50.105
    HostName jcanton.synology.me
    Port 9306
    IdentityFile ~/.ssh/id_rsa
    User jacopo
    ForwardX11 yes

Host ela
    HostName ela.cscs.ch
    User jcanton
    IdentityFile ~/.ssh/cscs-key
    ForwardX11 yes

Host balfrin* daint* tsa* santis* todi* eiger*
    Hostname %h.cscs.ch
    User jcanton
    IdentityFile ~/.ssh/cscs-key
    ProxyJump ela
    ForwardX11 yes

Host fog
    HostName fog2.ethz.ch
    #IdentityFile ~/.ssh/id_rsa
    User jcanton
    ForwardX11 yes

Host o3
    User jcanton
    Hostname o3.ethz.ch
    ProxyCommand ssh -q -W %h:%p fog

Host argon
    User jcanton
    Hostname argon.ethz.ch
    ProxyCommand ssh -q -W %h:%p fog

Host co2
