Host *
    ServerAliveInterval 120
    TCPKeepAlive no
    ForwardX11 yes

Host pi
    HostName 192.168.1.95
    IdentityFile ~/.ssh/id_rsa
    User pi

Host ciccia
    HostName jcanton.synology.me
    Port 9306
    IdentityFile ~/.ssh/id_rsa
    User jacopo

Host ela
    HostName ela.cscs.ch
    User jcanton
    IdentityFile ~/.ssh/cscs-key

Host balfrin
    Hostname balfrin-ln003.cscs.ch
    User jcanton
    IdentityFile ~/.ssh/cscs-key
    ProxyJump ela

Host tsa daint* santis* todi* eiger*
    Hostname %h.cscs.ch
    User jcanton
    IdentityFile ~/.ssh/cscs-key
    ProxyJump ela

Host fog
    HostName fog2.ethz.ch
    IdentityFile ~/.ssh/id_rsa
    User jcanton

Host o3 argon co2
    User jcanton
    Hostname %h.ethz.ch
    ProxyJump fog
