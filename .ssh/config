Host *
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_rsa
    ServerAliveInterval 120
    TCPKeepAlive no

Host pi
    HostName 192.168.1.102
    IdentityFile ~/.ssh/id_rsa
    User pi
    ForwardX11 yes

Host ela
    HostName ela.cscs.ch
    IdentityFile ~/.ssh/id_rsa
    User jcanton
    ForwardX11 yes

Host daint*.cscs.ch
    ForwardAgent yes
    Port 22
    ProxyCommand ssh -q -Y jcanton@ela.cscs.ch netcat %h %p -w 10
    ForwardX11 yes

Host daint
    Hostname daint.cscs.ch
    User jcanton
    ForwardAgent yes
    Port 22
    ProxyCommand ssh -q -Y jcanton@ela.cscs.ch netcat %h %p -w 10
    ForwardX11 yes

Host fog
    HostName fog2.ethz.ch
    #IdentityFile ~/.ssh/id_rsa
    User jcanton
    ForwardX11 yes

Host o3
    User jcanton
    Hostname o3.ethz.ch
    ProxyCommand ssh -q -W %h:%p fog

Host argon
    User jcanton
    Hostname argon.ethz.ch
    ProxyCommand ssh -q -W %h:%p fog

Host erebos
    Hostname erebos.mech.kth.se
    User jcanton
    ForwardX11 yes

Host eulerD
    Hostname euler.ethz.ch
    User jcanton
    ForwardX11 yes

Host euler
    Hostname 129.132.93.102
    User jcanton
    ForwardX11 yes
