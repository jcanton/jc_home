__conda_setup="$('/Users/jack/miniconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    #eval "$__conda_setup"
    export PATH="/Users/jack/miniconda3/bin:$PATH"
else
    if [ -f "/Users/jack/miniconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/jack/miniconda3/etc/profile.d/conda.sh"
